{
    "name": "eh3anr/laravel-otp",
    "description": "Laravel OTP generator and validation",
    "homepage": "https://github.com/eh3anr/laravel-otp",
    "keywords": [
        "laravel",
        "otp",
        "one time passwords",
        "one time pin",
        "email validation",
        "mobile validation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eh3an Rajabi",
            "email": "eh3an.r@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Eh3anr\\Otp\\OtpServiceProvider"
            ],
            "aliases": {
                "Otp": "Eh3anr\\Otp\\OtpFacade"
            }
        }
    },
    "require": {
        "php": "^5.5.9|^7.0|^8.0",
        "illuminate/support": "^5.2|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Eh3anr\\Otp\\": "src"
        }
    }
}
